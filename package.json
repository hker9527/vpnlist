{
  "name": "vpnlist-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "bun run prepare-css && vite build",
    "preview": "vite preview",
    "prepare-css": "bunx smui-theme compile static/smui.css -i src/theme && bunx smui-theme compile static/smui-dark.css -i src/theme/dark",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@smui-extra/accordion": "^7.0.0-beta.15",
    "@smui/button": "^7.0.0-beta.15",
    "@smui/data-table": "^7.0.0-beta.16",
    "@smui/drawer": "^7.0.0-beta.15",
    "@smui/icon-button": "^7.0.0-beta.15",
    "@smui/layout-grid": "^7.0.0-beta.15",
    "@smui/linear-progress": "^7.0.0-beta.16",
    "@smui/list": "^7.0.0-beta.15",
    "@smui/menu": "^7.0.0-beta.15",
    "@smui/select": "^7.0.0-beta.15",
    "@smui/snackbar": "^7.0.0-beta.15",
    "@smui/textfield": "^7.0.0-beta.16",
    "@smui/top-app-bar": "^7.0.0-beta.15",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.20.4",
    "@types/geojson": "^7946.0.14",
    "@types/svelte-leafletjs": "^0.8.5",
    "smui-theme": "^7.0.0-beta.15",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "svelte-leafletjs": "^0.10.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "type": "module",
  "dependencies": {
    "chart.js": "^4.4.1",
    "chartjs-chart-geo": "^4.2.8",
    "leaflet": "^1.9.4",
    "svgmap": "^2.10.1",
    "topojson-client": "^3.1.0",
    "zod": "^3.22.4"
  }
}
